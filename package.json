{
  "name": "sneakers-shop",
  "type": "module",
  "version": "1.0.0",
  "description": "ShoeMorph - Custom shoe sizing platform",
  "main": "index.js",
  "scripts": {
    "build-client": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "render-postbuild": "npm run install-client && npm run build-client",
    "client": "npm start --prefix client",
    "server": "node server/index.js",
    "render-start": "node server/index.js",
    "dev": "start cmd /c \"npm run server\" && cd client && npm start",
    "start": "node server/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "apollo-server": "^3.8.2",
    "apollo-server-express": "^3.8.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "graphql": "^16.5.0",
    "graphql-iso-date": "^3.6.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongoose": "^6.3.6",
    "react-redux": "^8.0.2"
  }
