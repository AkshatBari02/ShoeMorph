# ===========================================
# ShoeMorph Client Environment Variables
# ===========================================
# Copy this file to .env and fill in your values

# Backend GraphQL URL
REACT_APP_GRAPHQL_URL=http://localhost:5000/graphql

# Foot Size Measurement API URL
REACT_APP_FOOT_MEASUREMENT_API_URL=http://localhost:8000

# PayPal Configuration
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_client_id_here

# Cloudinary Configuration
REACT_APP_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
REACT_APP_CLOUDINARY_UPLOAD_PRESET=your_cloudinary_upload_preset
